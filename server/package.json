{
  "name": "collectible-pleasures-server",
  "buildCommand": "npm run vercel-build",
  "version": "1.0.0",
  "scripts": {
    "sketch": "browser-sync start --proxy localhost:8080 --files 'public/**/*'",
    "server": "nodemon index.js",
    "start": "concurrently \"npm run server\" \"npm run sketch\"",
    "vercel-build": "echo \"collectible pleasures\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "browser-sync": "^2.29.1",
    "concurrently": "^8.0.1",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.22"
  }
}