{
    "version": 2,
    "builds": [
        {
            "src": "/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "public/letters.js",
                    "public/sketch.html",
                    "public/sketch.js",
                    "public/style.css"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}