{
    "version": 2,
    "devCommand": "nodemon index.js",
    "framework": null,
    "installCommand": "npm install",
    "public": true,
    "headers": [
        {
            "source": "/",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ],
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/*",
            "use": "@vercel/static"
        }
    ]
}